target 'iosMaggioliEbookApp' do
  use_frameworks!
  platform :ios, '14.1'
  pod 'shared', :path => '../shared'
  pod 'KMPNativeCoroutinesRxSwift'

  # Readium swift-toolkit
  pod 'R2Shared'
  pod 'R2Streamer'
  pod 'R2Navigator'
  pod 'ReadiumOPDS'
  
  # Required for R2Streamer.
  pod 'GCDWebServer'
end
